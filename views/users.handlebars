{{#> navbar}}
<h1 class="title">
    Users
</h1>

{{#if users}}
<table class="table is-hoverable is-fullwidth">
    <tbody>
        {{#each users}}
        <tr>
            <td>
                <a href="/my-music/{{user_id}}">
                    {{username}}
                </a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{else}}
<p>There is no users registered yet</p>
{{/if}}
{{/navbar}}